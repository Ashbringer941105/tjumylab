 "http://struts.apache.org/dtds/struts-2.3.dtd" >  
<struts>  
    <package name="default" extends="struts-default">  
        <action name="index" class="com.lab.action.IndexAction">  
            <result>/index.jsp</result>  
        </action> 
        
        <action name="activity" class="com.lab.action.ActivityAction" >  
            <result>/activity.jsp</result>  
        </action> 
        
        <action name="memberlist" class="com.lab.action.MemberlistAction">  
